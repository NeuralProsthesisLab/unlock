<?xml version="1.0" encoding="UTF-8"?>
<objects xmlns="http://www.springframework.org/springpython/schema/objects"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/springpython/schema/objects
       		http://springpython.webfactional.com/schema/context/spring-python-context-1.0.xsd">
			
	<object id="window" class="unlock.controller.PygletWindow">
		<constructor-arg name="fullscreen"><value>False</value></constructor-arg>
		<constructor-arg name="show_fps"><value>True</value></constructor-arg>
	</object>

	<object id="collector_single_centered_msequence_collector" class="controller.Collector" factory_method="create_single_centered_msequence_collector">
		<constructor-arg><ref object="window" /></constructor-arg>
		<constructor-arg name="standalone"><value>False</value></constructor-arg>
		<constructor-arg name="rest"><value>4</value></constructor-arg>
	</object>
	
	<object id="standalone_collector_single_centered_msequence_collector" class="controller.Collector" factory_method="create_single_centered_msequence_collector">
		<constructor-arg><ref object="window" /></constructor-arg>
		<constructor-arg name="rest"><value>4</value></constructor-arg>
	</object>
	
	<object id="collector_msequence_collector" class="controller.Collector" factory_method="create_msequence_collector">
		<constructor-arg><ref object="window" /></constructor-arg>
		<constructor-arg name="standalone"><value>False</value></constructor-arg>
	</object>
	
	<object id="standalone_collector_msequence_collector" class="controller.Collector" factory_method="create_msequence_collector">
		<constructor-arg><ref object="window" /></constructor-arg>
	</object>	
	
</objects>

